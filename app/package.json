{
  "name": "web",
  "version": "1.0.0",
  "description": "Project web",
  "main": "index.js",
  "scripts": {
    "__build": "webpack",
    "__server": "node ./dist/server.js",
    "__watch": "npm run build && npm run server",
    "__dev": "nodemon --watch src --ext ts,tsx,css --exec \"npm run watch\"",
    "ssr:dev:build": "webpack --config ./config/webpack/ssr/config.dev.js",
    "ssr:server": "node ./dist/server.js",
    "watch:server": "npm run ssr:dev:build && npm run ssr:server",
    "start:server": "nodemon --watch src --ext ts,tsx --exec \"npm run watch:server\"",
    "start:browser": "webpack-dev-server --config ./config/webpack/browser/config.dev.js --host 0.0.0.0",
    "start:1": "npm run start:browser",
    "ssr:build:dev": "webpack --config ./config/webpack/ssr/config.dev.js",
    "1start:build": "npm run browser:build:dev && npm run ssr:build:dev",
    "1start": "node ./dist/index.js",
    "1server:watch": "nodemon --watch src --ext ts,tsx --exec \"npm run start:build\"",
    "1browser:watch": "webpack-dev-server --config ./config/webpack/site/config.browser.dev.js --host 0.0.0.0",
    "---browser:build:dev": "webpack --config ./config/webpack/browser/config.dev.js",
    "browser:compiler": "webpack --config ./config/webpack/site/config.browser.dev.js",
    "browser:watch": "webpack --watch --config ./config/webpack/site/config.browser.dev.js",
    "server:watch": "webpack --watch --config ./config/webpack/site/config.server.js",
    "server:compiler": "webpack --config ./config/webpack/site/config.server.js",
    "all": "npm run build:server:compiler && npm run build:browser:dev && npm run start:watch & npm run build:server:watch",
    "compiler": "npm run build:server:compiler && npm run build:browser:dev",
    "xxxxbuild:site:dev": "npm run build:browser:dev && npm run build:server",
    "build": "rm -fr ./dist && npm run browser:compiler && npm run server:compiler",
    "nodemon": "nodemon --ignore ./src ./dist/index.js",
    "start:all": "npm run nodemon & npm run server:watch & npm run browser:watch"
  },
  "author": "Jhonnatan Castro",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^2.0.15",
    "react-query": "^3.34.16",
    "axios": "^0.26.0",
    "@types/react-router-dom": "^5.3.3",
    "file-loader": "^6.2.0",
    "ignore-loader": "^0.1.2",
    "dotenv-expand": "^5.1.0",
    "dotenv-webpack": "^7.0.2",
    "webpack-merge": "^5.8.0",
    "node-sass": "^7.0.1",
    "sass-loader": "^12.6.0",
    "postcss-loader": "^6.2.1",
    "mysql": "^2.17.1",
    "webpack-manifest-plugin": "^5.0.0",
    "express-manifest-helpers": "^0.6.0",
    "css-hot-loader": "^1.4.4",
    "react-helmet-async": "^1.2.3"
  },
  "devDependencies": {
    "@types/mysql": "^2.15.8",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "core-js": "^3.21.1",
    "regenerator-runtime": "^0.13.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "css-loader": "^5.2.6",
    "mini-css-extract-plugin": "^2.5.3",
    "clean-webpack-plugin": "^4.0.0",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-dev-server": "^4.7.4",
    "@types/body-parser": "^1.17.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/node": "^13.1.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "ts-loader": "^8.0.18",
    "style-loader": "^3.3.1",
    "typescript": "^4.2.4",
    "@types/react-helmet": "^6.1.5"
  }
}
